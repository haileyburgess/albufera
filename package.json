{
  "name": "albufera",
  "type": "module",
  "imports": {
    "#*": "./*.js"
  },
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "node --watch --env-file=.env server.js",
    "test": "vitest",
    "db:schema": "psql -d albufera -f db/schema.sql",
    "db:seed": "node --env-file=.env db/seed.js",
    "db:reset": "npm run db:schema && npm run db:seed",
    "build": "vite build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-data-grid": "^8.10.0",
    "add-to-calendar-button": "^2.9.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "^7.7.1",
    "react-router-dom": "^7.7.1",
    "react-social-media-embed": "^2.5.18"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "vitest": "^3.1.3"
  }
}
